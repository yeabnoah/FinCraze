{
  "expo": {
    "scheme": "acme",
    "userInterfaceStyle": "automatic",
    "orientation": "default",
    "web": {
      "output": "static",
      "bundler": "metro"
    },
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "plugins": [
      ["expo-router"],
      [
        "expo-font",
        {
          "fonts": ["./src/assets/fonts/"]
        }
      ]
    ],
    "name": "fincraze-SENDER",
    "slug": "fincraze",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c95f39ea-76fb-453d-ae2c-6e1fb7b5b4cb"
      }
    },
    "android": {
      "package": "com.technerd345.fincraze"
    }
  }
}
